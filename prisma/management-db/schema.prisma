generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("MANAGEMENT_DATABASE_URL")
}

model Project {
  projectId         Int    @id @default(autoincrement())
  projectName       String
  dbName            String @unique
  members           Member[]
}

model Member {
  projectId         Int
  email             String
  @@id([projectId, email])
  project           Project @relation(fields: [projectId], references: [projectId])
}
