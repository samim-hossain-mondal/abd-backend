generator client {
  provider = "prisma-client-js"
  output   = "../../src/generated/management"
}

datasource db {
  provider = "postgresql"
  url      = env("MANAGEMENT_DATABASE_URL")
}

model Project {
  projectId          Int      @id
  projectName        String
  projectDescription String?
  members            Member[]
}

model Member {
  projectId Int
  email     String
  project   Project @relation(fields: [projectId], references: [projectId])

  @@id([projectId, email])
}
